<h2>Conference Rooms</h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Sq ft</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.location %></td>
        <td><%= room.sq_ft %></td>
        <td><%= room.description %></td>
        <td><%= link_to 'View Photos', room_photos_path(room) %></td>
        <td><button class="tiny success button">Reserve</button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

